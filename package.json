{
  "name": "server-site",
  "version": "1.0.0",
  "description": "THL site server",
  "main": "server/index.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build:svg": "export PATH=$(npm bin):$PATH && svg-sprite --config svg-sprite.config.json ./src/svg/*.svg",
    "build:jenkins": "chmod a+rx ./script/build-jenkins.sh && ./script/build-jenkins.sh",
    "build": "export PATH=$(npm bin):$PATH && npm run clean && npm run build:svg && webpack --config webpack.config.js",
    "build:prod": "export NODE_ENV=production && npm run build",
    "serve": "export NODE_ENV=development && babel-node .",
    "start": "npm run build && npm run serve",
    "test": "jest --config=jest.config.js",
    "lint": "eslint \"src/**/*.{js,jsx}\"",
    "watch:build": "npm run build -- --watch",
    "watch:serve": "nodemon --exec \"npm run serve\"",
    "watch:test": "npm run test -- --watch",
    "watch": "npm-run-all --parallel watch:build watch:serve",
    "report:test:coverage": "npm run test -- --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Tonghuilin/server-site.git"
  },
  "keywords": [
    "thl"
  ],
  "author": "Chi Yan",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Tonghuilin/server-site/issues"
  },
  "homepage": "https://github.com/Tonghuilin/server-site#readme",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@emotion/core": "^10.0.5",
    "@emotion/styled": "^10.0.5",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0-beta.6",
    "babel-plugin-emotion": "^10.0.0",
    "babel-plugin-file-loader": "^1.1.1",
    "chalk": "^2.4.1",
    "emotion": "^10.0.0",
    "emotion-server": "^10.0.0",
    "emotion-theming": "^10.0.5",
    "eslint": "^5.10.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "express": "^4.16.4",
    "glob": "^7.1.3",
    "handlebars": "^4.0.12",
    "jest": "^23.6.0",
    "nodemon": "^1.18.7",
    "npm-run-all": "^4.1.5",
    "svg-sprite": "^1.5.0",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "yargs": "^12.0.5"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "date-fns": "^1.29.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2"
  }
}
